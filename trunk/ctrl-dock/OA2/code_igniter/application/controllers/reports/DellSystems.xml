<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Dell Systems</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, oa_org.org_name, system.man_ip_address, system.man_serial, floor(system.pc_memory / 1024) as pc_memory, system.man_model, system.pc_date_os_installation, sys_sw_windows.windows_client_site_name FROM system LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id LEFT JOIN sys_sw_windows ON (system.system_id = sys_sw_windows.system_id AND system.timestamp = sys_sw_windows.timestamp) LEFT JOIN oa_org ON system.man_org_id = oa_org.org_id WHERE oa_group_sys.group_id = @group AND system.man_manufacturer LIKE '%Dell%' GROUP BY system.hostname ORDER BY system.hostname]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
	</details>
