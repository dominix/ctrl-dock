		<report_name>Web Sites</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT DISTINCT(system.system_id), system.hostname, system.man_os_family, system.man_environment, sys_sw_web_site.site_state, sys_sw_web_server.webserver_version, sys_sw_web_site.site_description, sys_sw_web_site.site_path, sys_sw_web_site.site_app_pool, sys_sw_web_site.site_logging, sys_sw_web_site.site_log_format, sys_sw_web_site.site_log_directory, sys_sw_web_site.site_log_rotation FROM system, oa_group_sys, sys_sw_web_server, sys_sw_web_site WHERE system.man_status = "production" AND system.system_id = oa_group_sys.system_id AND oa_group_sys.group_id = @group AND system.timestamp = sys_sw_web_site.timestamp AND system.system_id = sys_sw_web_server.system_id AND sys_sw_web_server.webserver_id = sys_sw_web_site.webserver_id ORDER BY system.hostname, sys_sw_web_site.site_description]]></report_sql>
