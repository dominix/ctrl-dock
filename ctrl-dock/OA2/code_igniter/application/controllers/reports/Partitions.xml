<report>
	<details>
		<report_name>Partitions</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT DISTINCT(system.system_id), system.hostname, sys_hw_partition.partition_name, sys_hw_partition.partition_mount_point, system.man_environment, system.man_function, system.man_criticality, sys_hw_partition.partition_size, sys_hw_partition.partition_used_space, sys_hw_partition.partition_free_space, ROUND(((sys_hw_partition.partition_free_space / sys_hw_partition.partition_size) * 100), 0) AS percent_free FROM system LEFT JOIN sys_hw_partition ON (system.timestamp = sys_hw_partition.timestamp AND system.system_id = sys_hw_partition.system_id) LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id WHERE oa_group_sys.group_id = @group AND sys_hw_partition.partition_mount_type = "partition" ORDER BY system.hostname, sys_hw_partition.partition_mount_point]]></report_sql>
